permalink: recipe-list.json
---
{"recipes" : [
{%- for item in collections.recipes -%}
  {
    "url" : "{{ item.url }}",
    "icon" : "{{ item.data.icon }}",
    "ingredients" : [ {% for ing in item.data.ingredients %}"{{ ing }}"{% if not loop.last %},{% else %}{%- endif -%}{% endfor %}],
    "title" : "{{ item.data.title }}"
  }{% if not loop.last %},{% else %}{%- endif -%}
{%- endfor -%}
]}